{
  "name": "yterasaka/japanese-language-pack",
  "description": "Japanese language pack for Shopware 6",
  "type": "shopware-platform-plugin",
  "version": "1.0.0",
  "license": "MIT",
  "require": {
    "shopware/core": "~6.7.0"
  },
  "extra": {
    "shopware-plugin-class": "JapaneseLanguagePack\\JapaneseLanguagePack",
    "label": {
      "de-DE": "Japanisches Sprachpaket",
      "en-GB": "Japanese Language Pack",
      "ja-JP": "日本語言語パック"
    }
  },
  "autoload": {
    "psr-4": {
      "JapaneseLanguagePack\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JapaneseLanguagePack\\Tests\\": "tests/"
    }
  }
}
